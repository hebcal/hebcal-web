{
  "name": "hebcal-web",
  "version": "3.5.2",
  "description": "Hebcal server-side Node.js for www.hebcal.com and download.hebcal.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebcal/hebcal-web.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-web/issues"
  },
  "homepage": "https://github.com/hebcal/hebcal-web#readme",
  "config": {
    "sprite": "sprite13.svg",
    "csprite": "color-icons4.svg",
    "mainCss": "style-5.3.8d.css",
    "clientapp": "hebcal-app-5.2.1.min.js"
  },
  "type": "module",
  "engines": {
    "node": ">= 24"
  },
  "files": [],
  "scripts": {
    "dev": "node src/app-www.js",
    "dev:download": "NODE_PORT=8081 node src/app-download.js",
    "po2json": "node ./po2json.js po/he.po po/pt.po po/es.po po/fr.po po/ashkenazi.po",
    "build": "npm run po2json && npm run css-compile && npm run css-rename && npx rollup -c",
    "deploy": "./deploy.sh",
    "start": "node src/app-www.js",
    "protoc": "protoc --proto_path=src --js_out=import_style=commonjs,binary:src src/download.proto && mv src/download_pb.js src/download_pb.cjs",
    "css-compile": "sass --style compressed --embed-sources --no-error-css --load-path=node_modules --load-path=views/partials scss/:css/",
    "css-rename": "mv css/style.css static/i/$npm_package_config_mainCss",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "hebcal"
  ],
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@popperjs/core": "^2.11.8",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "bootstrap": "^5.3.8",
    "bootstrap5-autocomplete": "^1.1.42",
    "csv-parse": "^6.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-n": "^17.23.2",
    "gettext-parser": "^9.0.1",
    "globals": "^17.0.0",
    "rollup": "^4.56.0",
    "sass": "^1.97.3",
    "supertest": "^7.2.2",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@echo-health/koa-prometheus-exporter": "^1.0.4",
    "@hebcal/core": "^6.0.8",
    "@hebcal/geo-sqlite": "^5.7.2",
    "@hebcal/hdate": "^0.21.1",
    "@hebcal/icalendar": "^6.3.4",
    "@hebcal/learning": "^6.6.1",
    "@hebcal/leyning": "^9.4.0",
    "@hebcal/locales": "^6.3.3",
    "@hebcal/rest-api": "^6.4.0",
    "@hebcal/triennial": "^6.2.0",
    "@koa/ejs": "^5.1.0",
    "@koa/send": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "dayjs": "^1.11.19",
    "ejs": "^3.1.10",
    "emoji-flag": "^1.1.0",
    "fastest-levenshtein": "^1.0.16",
    "geo-tz": "^8.1.5",
    "geolite2-redist": "^3.1.1",
    "google-protobuf": "^4.0.1",
    "haversine": "^1.1.1",
    "http-errors": "^2.0.1",
    "ini": "^6.0.0",
    "koa": "^3.1.1",
    "koa-better-response-time": "^1.3.0",
    "koa-bodyparser": "^4.4.1",
    "koa-compress": "^5.1.1",
    "koa-conditional-get": "^3.0.0",
    "koa-error": "^3.2.0",
    "koa-static": "^5.0.0",
    "koa-timeout-v2": "^1.0.0",
    "maxmind": "^5.0.3",
    "minimist": "^1.2.8",
    "murmurhash3": "^0.5.0",
    "mysql2": "^3.16.1",
    "nodemailer": "^7.0.12",
    "nodemailer-html-to-text": "^3.2.0",
    "pdfkit": "^0.17.2",
    "pino": "^10.2.1",
    "probe-image-size": "^7.2.3",
    "quick-lru": "^7.3.0",
    "random-bigint": "0.0.1",
    "transliteration": "^2.6.1",
    "ulid": "^3.0.2"
  }
}
